<% var headerText = ['Home', 'Blog', 'Come Visit!']; %>
<% var headerLinks = ['/', '/blog', '/visit']; %>

<div class="container-fluid">
  <div class="row">
    <% for(var i=0; i<headerLinks.length; i++) { %>
		<div class="col-md-1">
			<a href='<%= headerLinks[i] %>'><%= headerText[i] %></a>
		</div>
	<% } %>
  </div>
</div>